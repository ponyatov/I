# comment

files = {
	src.src log.log
	core  = [
		ypp.ypp		" syntax parser "
		lpp.lpp		" lexer "
		hpp.hpp		" headers "
		cpp.cpp		" Dynamic Language Runtime "
		Makefile	" build script "
	]
	gvim = [ rc.rc bat.bat ]	" (g)vim helpers "
}
touch @ files

primitive = {
    symbols = [ sym bols ]
    numbers = {
        hex = 0xDeadBeef
        integer = -01
        number = [ +2.3 ; -04e+05 ]
    }
    'string'
}

quad = { X: X*X }	" function sample "

quad @ {X=2}

